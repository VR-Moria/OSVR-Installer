{
  "server": {
     "sleep": 0
  },
  "externalDevices": {
    "/myExternalOculus": {
      "deviceName": "Tracker0",
      "server": "localhost:3884",
      "descriptor": { /* can also provide a file path here, or this is the minimal */
        "interfaces": {
          "tracker": {
            "count": 1,
            "bounded": true,
            "position": false,
            "orientation": true,
            "angularVelocity": true
          }
        }
      }
    },
    "/myExternalXbox": {
      "deviceName": "xbox0",
      "server": "localhost:3884",
      "descriptor": {
        "interfaces": {
                  "analog": {
                        "count": 5
                  },
                  "button": {
                        "count": 8
                  }
        },
	"semantic": {
		"1": "button/0",
		"2": "button/1",
		"3": "button/2",
		"4": "button/3",
		"5": "button/4",
		"6": "button/5",
		"7": "button/6",
		"8": "button/7",
		"leftStickX": "analog/0",
		"leftStickY": "analog/1",
		"rightStickX": "analog/2",
		"rightStickY": "analog/3",
		"trigger": "analog/4"
        },
        "automaticAliases": {
                "/controller": "semantic/*"
        }
      }
    }
  },
  "aliases": {
    "/me/head": {
        "translate": [0.0, 0.0, -0.06],
        "child": "/myExternalOculus/tracker/0"
    }
  },
	"renderManagerConfig": {
		"meta": {
			"schemaVersion": 1
		},
		"renderManagerConfig": {
			"directModeEnabled": false,
			"directDisplayIndex": 0,
			"directHighPriorityEnabled": true,
			"numBuffers": 2,
			"verticalSyncEnabled": false,
			"verticalSyncBlockRenderingEnabled": true,
			"renderOverfillFactor": 2.0,
			
			"window": {
				"title": "UMoriaVR",
				"fullScreenEnabled": false,
				"xPosition": 10,
				"yPosition": 40
			},

			"display": {
				"rotation": 0,
				"bitsPerColor": 8
			},

			"prediction": {
					"enabled": true,
					"staticDelayMS": 20,
					"leftEyeDelayMS": 5,
					"rightEyeDelayMS": 0,
					"localTimeOverride": true
			},

			"timeWarp": {
				"enabled": true,
				"asynchronous": false,
				"maxMsBeforeVSync": 5
			}
		}
	},

	"display": {
		"meta": {
			"schemaVersion": 1
		},
		"hmd": {
			"device": {
				"properties": {
					"vendor": "Oculus",
					"model": "DK2",
					"num_displays": 1,
					"Version": "1.1",
					"Note": "UMoriaVR"
				}
			},
			"field_of_view": {
                "monocular_horizontal": 83,
                "monocular_vertical": 92,
				"overlap_percent": 100,
				"pitch_tilt": 0
			},
			"resolutions": [
				{
				"width": 1920,
				"height": 1080,
				"video_inputs": 1,
				"display_mode": "horz_side_by_side",
				"swap_eyes": 0
				}
			],
			"distortion": {
				"distance_scale_x": 1,
				"distance_scale_y": 1,
				"polynomial_coeffs_red": [ 0, 1 ],
				"polynomial_coeffs_green": [ 0, 1 ],
				"polynomial_coeffs_blue": [ 0, 1 ]
			},
			"rendering": {
				"right_roll": 0,
				"left_roll": 0
			},
			"eyes": [
				{
				"center_proj_x": 0.5,
				"center_proj_y": 0.5,
				"rotate_180": 0
				},
				{
				"center_proj_x": 0.5,
				"center_proj_y": 0.5,
				"rotate_180": 0
				}
			]
			}
		}
}
